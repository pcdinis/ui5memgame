<mvc:View
    controllerName="pt.pcdinis.ui5memgame.ui5memgame.controller.Game"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
	xmlns:core="sap.ui.core"
	xmlns:card="sap.f.cards"
	xmlns:l="sap.ui.layout"
    displayBlock="true"
    xmlns="sap.m"
>

	<Page id="page">
		<subHeader>
			<Toolbar>
				<Button type="Back" tooltip="Back" press="onPressBack"/>
				<ToolbarSpacer/>
				<Title text="{i18n>title}" level="H3"/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>

		<Input id="timer" editable="false" value="" width="100%"/>

		<Panel id="panelForGridList" width="auto" class="sapUiTinyMarginEnd">
		</Panel>

		<content>
		</content>
	</Page>
</mvc:View>
